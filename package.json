{
  "name": "microview-ai",
  "version": "1.0.0",
  "description": "MicroView AI: Leveraging Large Vision-Language Models in an Augmentative Raspberry Pi-Based System for Automated Urine Microscopy Analysis",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "eslint --fix \"**/*.{ts,tsx,js,jsx}\"",
    "export": "next build && next export",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "npm run build && electron-builder",
    "electron-pack-linux": "npm run build && electron-builder --linux",
    "electron-pack-arm": "npm run build && electron-builder --linux --armv7l",
    "dist": "npm run build && electron-builder --publish=never",
    "dist-linux": "npm run build && electron-builder --linux --publish=never",
    "dist-arm": "npm run build && electron-builder --linux --armv7l --publish=never"
  },
  "build": {
    "appId": "com.microview.ai",
    "productName": "MicroView AI",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      "out/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "armv7l"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64",
            "armv7l"
          ]
        }
      ],
      "category": "Science",
      "icon": "electron/assets/icon.png"
    },
    "extraResources": [
      {
        "from": "electron/assets",
        "to": "assets"
      }
    ]
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.39.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.480.0",
    "next": "15.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^3.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-is-dev": "^2.0.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "15.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.0",
    "wait-on": "^7.2.0"
  },
  "keywords": [
    "microscope",
    "urinalysis",
    "ai",
    "raspberry-pi",
    "medical",
    "automation",
    "computer-vision"
  ],
  "author": "BS Computer Engineering Thesis Project - Pamantasang ng Lungsod ng Maynila",
  "license": "MIT"
}
